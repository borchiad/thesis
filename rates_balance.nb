  RowBox[{"VoltageSolver", "[", 
   RowBox[{
   "V_", ",", "\[Phi]a_", ",", "\[Phi]b_", ",", "T_", ",", "NumStepC_"}], 
   "]"}], ":=", 
  RowBox[{
   RowBox[{"VoltageSolver", "[", 
    RowBox[{"V", ",", "\[Phi]a", ",", "\[Phi]b", ",", "T", ",", "NumStepC"}], 
    "]"}], "=", "\n", "\t", 
   RowBox[{"Module", "[", "\n", "\t\t", 
    RowBox[{
     RowBox[{"{", "\n", "\t\t\t", 
      RowBox[{"LeftVoltageTab", ",", "RightVoltageTab", ",", " ", 
       RowBox[{"(*", 
        RowBox[{"voltage", " ", "tables"}], "*)"}], "\n", "\t\t\t", 
       "DifferenceTable", ",", "\n", "\t\t\t", "LeftVoltage", ",", 
       "RightVoltage"}], " ", 
      RowBox[{"(*", 
       RowBox[{"numeric", " ", "values"}], "*)"}], "\n", "\t\t", "}"}], ",", 
     "\n", "\t\t", 
     RowBox[{
      RowBox[{"LeftVoltageTab", "=", 
       RowBox[{"Subdivide", "[", 
        RowBox[{"0", ",", "V", ",", "NumStepC"}], "]"}]}], ";", "\n", "\t\t", 
      RowBox[{"LeftVoltageTab", "=", 
       RowBox[{"Subdivide", "[", 
        RowBox[{"V", ",", "0", ",", "NumStepC"}], "]"}]}], ";", "\n", "\t\t", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{
        "siingle", " ", "value", " ", "of", " ", "temperature", " ", 
         "comes"}], ",", " ", 
        RowBox[{
        "but", " ", "we", " ", "need", " ", "two", " ", "fluxes", " ", "for", 
