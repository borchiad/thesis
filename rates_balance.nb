Cell[BoxData[
 RowBox[{
  RowBox[{"Solver", "[", 
   RowBox[{"V_", ",", "T_", ",", "\[Phi]a_", ",", "\[Phi]b_"}], "]"}], ":=", 
  "\n", 
  RowBox[{"Module", "[", "\n", "\t\t", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "BegVl", ",", "BegVr", ",", "LastVl", ",", "LastVr", ",", "BegT", ",", 
      "LastT"}], "}"}], ",", "\n", "\t\t\t", 
    RowBox[{
     RowBox[{"BegVl", "=", 
      RowBox[{"V", "/", "2"}]}], ";", "\n", "\t\t\t", 
     RowBox[{"BegVr", "=", 
      RowBox[{"V", "/", "2"}]}], ";", "\n", "\t\t\t", 
     RowBox[{"BegT", "=", "T"}], ";", "\n", "\t\t\t", 
     RowBox[{"Do", "[", "\n", "\t\t\t\t", 
      RowBox[{
       RowBox[{"If", "[", "\n", "\t\t\t\t\t", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"Abs", "[", 
            RowBox[{"LastVl", "-", "BegVl"}], "]"}], ">", "ThreV"}], "  ", "&&",
           " ", 
          RowBox[{
           RowBox[{"Abs", "[", 
            RowBox[{"LastT", "-", "BegT"}], "]"}], ">", "ThreT"}]}], ",", 
         "\n", "\t\t\t\t\t", 
         RowBox[{"{", "\n", "\t\t\t\t\t\t", 
          RowBox[{
           RowBox[{"LastVl", "=", "BegVl"}], ";", "\n", "\t\t\t\t\t\t", 
           RowBox[{"LastVr", "=", "BegVr"}], ";", "\n", "\t\t\t\t\t\t", 
           RowBox[{"LastT", "=", "BegT"}], ";", "\n", "\t\t\t\t\t\t", 
           RowBox[{"BegVl", "=", 
            RowBox[{
             RowBox[{"VoltageSolver", "[", 
              RowBox[{
              "LastVl", ",", "\[Phi]a", ",", "\[Phi]b", ",", "LastT", ",", 
               "NumStepC"}], "]"}], "[", 
             RowBox[{"[", "1", "]"}], "]"}]}], ";", "\n", "\t\t\t\t\t\t", 
           RowBox[{"BegVr", "=", 
            RowBox[{
             RowBox[{"VoltageSolver", "[", 
              RowBox[{
              "LastVl", ",", "\[Phi]a", ",", "\[Phi]b", ",", "LastT", ",", 
               "NumStepC"}], "]"}], "[", 
             RowBox[{"[", "2", "]"}], "]"}]}], ";", "\n", "\t\t\t\t\t\t", 
           RowBox[{"BegT", "=", 
            RowBox[{"TemperatureSolver", "[", 
             RowBox[{
             "LastVl", ",", "LastVr", ",", "\[Phi]a", ",", "\[Phi]b", ",", 
              "NumStepT"}], "]"}]}], ";"}], "\n", "\t\t\t\t\t", "}"}], ",", 
         "\n", "\t\t\t\t\t", 
         RowBox[{"{", "\n", "\t\t\t\t\t\t", 
          RowBox[{"Break", ";"}], "\n", "\t\t\t\t\t", "}"}]}], "\n", 
        "\t\t\t\t", "]"}], ",", "\n", "\t\t\t\t", 
       RowBox[{"{", 
        RowBox[{"k", ",", "\[Infinity]"}], "}"}]}], "]"}], ";", "\n", 
     "\t\t\t", 
     RowBox[{"{", 
      RowBox[{"BegVl", ",", "BegVr", ",", "BegT"}], "}"}]}]}], "\n", 
