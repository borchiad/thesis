    RowBox[{
     RowBox[{"{", 
      RowBox[{"temporarytab", ",", "knee", ",", "peak"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "Position", " ", "of", " ", "knee", " ", "and", " ", "peak", " ", "in", 
       " ", "this", " ", "specific", " ", "flux", " ", "and", " ", 
       "temperature", " ", "configuration"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"knee", " ", "=", " ", 
       RowBox[{"TRefValKnee", "[", 
        RowBox[{
        "Ta", ",", "Tb", ",", "\[CapitalDelta]0a", ",", "\[CapitalDelta]0b", 
