Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"ProcedureTableJoiExt", "[", 
    RowBox[{
    "pts_", ",", "coarsepts_", ",", "medpts_", ",", "finepts_", ",", " ", 
     "fluxpts_", ",", "Ta_", ",", "Tb_", ",", "\[CapitalDelta]0a_", ",", 
     "\[CapitalDelta]0b_", ",", "FluxDependencea_", ",", "FluxDependenceb_", 
     ",", "Fluxa_", ",", "Fluxb_"}], "]"}], ":=", "\n", "\t", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"temporarytab", ",", "knee", ",", "peak"}], "}"}], ",", "\n", 
     "\t\t", 
     RowBox[{"(*", 
      RowBox[{
      "Position", " ", "of", " ", "knee", " ", "and", " ", "peak", " ", "in", 
       " ", "this", " ", "specific", " ", "flux", " ", "and", " ", 
       "temperature", " ", "configuration"}], "*)"}], "\n", "\t\t", 
     RowBox[{
      RowBox[{"knee", " ", "=", " ", 
       RowBox[{"TRefValKnee", "[", 
        RowBox[{
        "Ta", ",", "Tb", ",", "\[CapitalDelta]0a", ",", "\[CapitalDelta]0b", 
         ",", "FluxDependencea", ",", "FluxDependenceb", ",", "Fluxa", ",", 
         "Fluxb"}], "]"}]}], ";", "\n", "\t\t", 
      RowBox[{"peak", " ", "=", " ", 
       RowBox[{"TRefValPeak", "[", 
        RowBox[{
        "Ta", ",", "Tb", ",", "\[CapitalDelta]0a", ",", "\[CapitalDelta]0b", 
         ",", "FluxDependencea", ",", "FluxDependenceb", ",", "Fluxa", ",", 
         "Fluxb"}], "]"}]}], ";", "\n", "\t\t", 
      RowBox[{"temporarytab", " ", "=", " ", 
       RowBox[{"Join", "[", "\n", "\t\t\t\t", 
        RowBox[{
         RowBox[{"Subdivide", "[", 
          RowBox[{"beginning", ",", 
           RowBox[{
            RowBox[{"-", "knee"}], "-", "FineInterval"}], ",", 
           RowBox[{"pts", "*", "coarsepts"}]}], "]"}], ",", 
         RowBox[{"(*", 
          RowBox[{
           RowBox[{"First", " ", "subdivision"}], ",", " ", 
           RowBox[{
           "from", " ", "the", " ", "extreme", " ", "to", " ", "the", " ", 
            "knee"}], ",", " ", "excluded", ",", "\n", 
           "\t\t\t\t\t\t\t\t\t\t\t\t", 
           RowBox[{"containing", " ", "pts", "*", "coa", " ", 
            RowBox[{"points", ".", " ", "Asymptotic"}]}]}], "*)"}], "\n", 
         "\t\t\t\t", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"Abs", "[", 
             RowBox[{"knee", "-", "peak"}], "]"}], "\[LessEqual]", 
            RowBox[{"(", 
             RowBox[{"FineInterval", "+", "FineInterval"}], ")"}]}], ",", " ", 
           RowBox[{"(*", 
            RowBox[{
            "This", " ", "conidition", " ", "verifies", " ", "if", " ", "the",
              " ", "knee", " ", "and", " ", "the", " ", "peak", " ", "are", 
             " ", "TOO", " ", "CLOSE"}], "*)"}], "\n", "\t\t\t\t\t", 
           RowBox[{"{", "\n", "\t\t\t\t\t\t", 
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"Abs", "[", "peak", "]"}], "<", "FineInterval"}], ",", 
              " ", 
              RowBox[{"(*", 
               RowBox[{
                RowBox[{"This", " ", "is", " ", "a", " ", "condition", " ", 
                 RowBox[{"we", "'"}], "ll", " ", "se", " ", "later"}], ",", 
                " ", "\n", "\t\t\t\t\t\t\t\t\t\t", 
                RowBox[{
                "since", " ", "it", " ", "holds", " ", "in", " ", "various", 
                 " ", 
                 RowBox[{"situations", ".", " ", "Verifies"}], " ", "if", " ",
                  "the", " ", "peak", " ", "is", " ", "too", " ", "close", 
                 " ", "to", " ", "0"}]}], "*)"}], "\n", "\t\t\t\t\t\t\t", 
              RowBox[{"{", "\n", "\t\t\t\t\t\t\t\t", 
               RowBox[{"(*", 
                RowBox[{
